# {{ cookiecutter.project_name }} Environment Variables
# Copy this file to .env and update with your settings

{% if cookiecutter.include_neo4j == "yes" %}
# Neo4j
NEO4J_URI=bolt://localhost:7687
NEO4J_USERNAME=neo4j
NEO4J_PASSWORD=your_neo4j_password
{% endif %}

{% if cookiecutter.include_ai_agents == "yes" %}
# OpenAI API (for AI agents)
OPENAI_API_KEY=your_openai_api_key

# Anthropic API (alternative AI provider)
ANTHROPIC_API_KEY=your_anthropic_api_key
{% endif %}

{% if cookiecutter.include_trading == "yes" %}
# Exchange API Keys (Binance)
BINANCE_API_KEY=your_binance_api_key
BINANCE_API_SECRET=your_binance_api_secret
{% endif %}

# Log Level
LOG_LEVEL=INFO
